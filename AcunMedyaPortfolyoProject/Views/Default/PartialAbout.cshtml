@model  List<AcunMedyaPortfolyoProject.Models.About>
<!-- About Section -->
<section id="about" class="about section" style="background-color: #f2f6f9; ">

    <!-- Section Title -->
    <div class="container section-title" data-aos="fade-up">
        <h2>About Me</h2>
    </div><!-- End Section Title -->

    <div class="container" data-aos="fade-up" data-aos-delay="100">

        <div class="row gy-4 justify-content-center">
            <div class="col-lg-4">
                <img src=" @Model[0].ImageUrl" class="img-fluid" alt="">
            </div>
            <div class="col-lg-8 content">
                <h2>@Model[0].Title</h2>
                <p class="fst-italic py-3">
                    @Model[0].Decription1
                </p>
                <div class="row">
                    <div class="col-lg-6">
                        <ul>
                            <li><i class="bi bi-chevron-right"></i> <strong>Birthday:</strong> <span>@*@Model[0].BirthDay*@</span></li>
                            @*<li><i class="bi bi-chevron-right"></i> <strong>Website:</strong> <span>@Model[0].Website</span></li>*@
                            <li><i class="bi bi-chevron-right"></i> <strong>Phone:</strong> <span>@*@Model[0].Phone*@</span></li>
                            <li><i class="bi bi-chevron-right"></i> <strong>City:</strong> <span>@Model[0].City</span></li>
                        </ul>
                    </div>
                    <div class="col-lg-6">
                        <ul>
                            <li><i class="bi bi-chevron-right"></i> <strong>Age:</strong> <span>@Model[0].Age</span></li>
                            <li><i class="bi bi-chevron-right"></i> <strong>Degree:</strong> <span>@Model[0].Degree</span></li>
                            <li><i class="bi bi-chevron-right"></i> <strong>Email:</strong> <span>@Model[0].Email</span></li>
                            <li><i class="bi bi-chevron-right"></i> <strong>Freelance:</strong> <span>@Model[0].Freelance</span></li>
                        </ul>
                    </div>
                </div>
                <p class="py-3">
                    @Model[0].Decription2
                </p>
            </div>
        </div>

    </div>

</section><!-- /About Section -->
