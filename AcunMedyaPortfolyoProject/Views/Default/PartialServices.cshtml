@model List<AcunMedyaPortfolyoProject.Models.ServicesPart>
<section id="services" class="services section">

    <!-- Section Title -->
    <div class="container section-title" data-aos="fade-up">
        <h2>Services</h2>
        <p>Offering beginner-level programming solutions, web design, and technical support as I grow my skills in the tech industry.</p>
    </div><!-- End Section Title -->

    <div class="container">
        <div class="row gy-4">
            @foreach (var item in Model)
            {


                <div class="col-xl-3 col-md-6 d-flex" data-aos="fade-up" data-aos-delay="100">
                    <div class="service-item position-relative">
                        <div class="icon"><i class="@item.IconUrl"></i></div>
                        <h4><a href="" class="stretched-link">@item.Title</a></h4>
                        <p>@item.Description2</p>
                    </div>
                </div><!-- End Service Item -->


            }

        </div>


    </div>

</section>
