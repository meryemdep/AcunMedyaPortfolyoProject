
@{
    Layout = null;
}

<!doctype html>
<html lang="en">
<head>
   @Html.Action("PartialHead","AdminLayout")

</head>


<body>

    <div class="wrapper d-flex align-items-stretch">
        <nav id="sidebar" class="active" style="background: #800020;">
            <div class="custom-menu">
                <button type="button" id="sidebarCollapse" class="btn" style="border: 3px solid black; background: #800020; ">
                    <i class="fa fa-bars"></i>
                    <span class="sr-only">Toggle Menu</span>
                </button>
            </div>
            <div class="p-4">
                <h1><a href="index.html" class="logo">Flash</a></h1>
                @Html.Action("PartialMenu", "AdminLayout")
           
                <div class="footer">
                    <p>
                   You can directly add or delete what you have darling.
                    </p>
                </div>

            </div>
        </nav>

        <!-- RenderBody  -->
        <div id="content" class="p-4 p-md-5 pt-5">
           @RenderBody() 
        </div>
    </div>
    @Html.Action("PartialScript", "AdminLayout")

</body>
</html>